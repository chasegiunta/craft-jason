{
  "name": "chasegiunta/craft-jason",
  "description": "A field type for displaying & editing JSON data.",
  "type": "craft-plugin",
  "version": "3.0.1",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "jason"
  ],
  "support": {
    "docs": "https://github.com/chasegiunta/craft-jason/blob/master/README.md",
    "issues": "https://github.com/chasegiunta/craft-jason/issues"
  },
  "license": "proprietary",
  "authors": [
    {
      "name": "Chase Giunta",
      "homepage": "https://chasegiunta.com"
    }
  ],
  "require": {
    "craftcms/cms": "^4.0.0",
    "php": "^8.0.2"
  },
  "autoload": {
    "psr-4": {
      "chasegiunta\\jason\\": "src/"
    }
  },
  "extra": {
    "name": "Jason",
    "handle": "jason",
    "hasCpSettings": false,
    "hasCpSection": false,
    "changelogUrl": "https://raw.githubusercontent.com/chasegiunta/craft-jason/master/CHANGELOG.md",
    "class": "chasegiunta\\jason\\Jason"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "craftcms/rector": "dev-main"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}
